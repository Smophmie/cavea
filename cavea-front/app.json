{
  "expo": {
    "name": "Cavea",
    "slug": "cavea-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-fond-rouge.png",
    "scheme": "caveafront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cavea.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-fond-rouge.png",
        "backgroundColor": "#bb2700"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/cavea-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "084a1267-4a56-46ca-943e-1b2f64359d1f"
      }
    }
  }
}
